{
  "name": "awsaml",
  "version": "1.1.0",
  "description": "Periodically refreshes AWS access keys",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rapid7/awsaml.git"
  },
  "bugs": {
    "url": "https://github.com/rapid7/awsaml/issues"
  },
  "main": "app.js",
  "scripts": {
    "debug": "electron . --debug",
    "start": "node app.js",
    "app": "electron .",
    "prebuild": "rm -rf dist/",
    "build": "electron-packager . Awsaml --asar --ignore=node_modules/electron --ignore=dist --out=dist --platform=all --arch=x64 --version=0.37.6 --app-version=${npm_package_version} --app-bundle-id=com.rapid7.awsaml --helper-bundle-id=com.rapid7.awsaml.helper",
    "postbuild": "export platform=darwin; npm run zip & export platform=linux; npm run zip & export platform=win32; npm run zip",
    "zip": "cd dist/Awsaml-${platform}-x64 && zip -q -FS -r ../awsaml-v${npm_package_version}-${platform}-x64.zip .",
    "test": "mocha",
    "lint": "eslint *.js views/*/** lib/*/** test/**"
  },
  "dependencies": {
    "aws-sdk": "2.2.11",
    "body-parser": "1.14.1",
    "cookie-parser": "1.4.0",
    "errorhandler": "1.4.2",
    "express": "4.13.3",
    "express-react-views": "0.9.0",
    "express-session": "1.13.0",
    "ini": "1.3.4",
    "mkdirp": "^0.5.1",
    "morgan": "1.6.1",
    "passport": "0.3.0",
    "passport-saml": "0.13.0",
    "react": "0.14.1",
    "react-dom": "0.14.1",
    "xmldom": "0.1.19",
    "xpath.js": "1.0.6"
  },
  "devDependencies": {
    "electron-packager": "7.0.0",
    "electron-prebuilt": "0.37.6",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-config-rapid7": "0.0.15",
    "eslint-plugin-rapid7": "^5.0.1",
    "eslint-plugin-react": "^3.16.1",
    "mocha": "^2.4.5",
    "should": "^8.2.2"
  }
}
