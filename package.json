{
  "name": "awsaml",
  "version": "0.1.1",
  "description": "Periodically refreshes AWS access keys",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "app": "electron .",
    "prebuild:osx": "rm -f awsaml-v${npm_package_version}-darwin-x64.zip",
    "build:osx": "electron-packager . Awsaml --asar --overwrite --platform=darwin --arch=x64 --version=0.34.1 --app-version=${npm_package_version} --app-bundle-id=com.rapid7.awsaml --helper-bundle-id=com.rapid7.awsaml.helper",
    "postbuild:osx": "cd Awsaml-darwin-x64 && zip -q -r ../awsaml-v${npm_package_version}-darwin-x64.zip ."
  },
  "dependencies": {
    "aws-sdk": "2.2.11",
    "body-parser": "1.14.1",
    "cookie-parser": "1.4.0",
    "errorhandler": "1.4.2",
    "express": "4.13.3",
    "express-react-views": "0.9.0",
    "express-session": "1.12.0",
    "ini": "1.3.4",
    "morgan": "1.6.1",
    "passport": "0.3.0",
    "passport-saml": "0.13.0",
    "react": "0.14.1",
    "react-dom": "0.14.1",
    "xmldom": "0.1.19",
    "xpath.js": "1.0.6"
  }
}
